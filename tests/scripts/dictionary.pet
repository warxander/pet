var d = { };
d.v1 = "foo";
assert(d.v1 == "foo");
assert(d.v2 == null);

d.v1 = 4;
assert(d.v1 == 4);

d.f1 = fun(x) { return x + x; };
assert(d.f1(4) == 8);

var d1 = { };
d1.d = d;
d1.str = "foo";
